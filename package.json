{
  "name": "nsrtm-rate-payer-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve:single-spa:nsrtmRatePayerApp",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:single-spa:nsrtmRatePayerApp": "ng build nsrtmRatePayerApp --configuration production",
    "serve:single-spa:nsrtmRatePayerApp": "ng s --project nsrtmRatePayerApp --disable-host-check --port 60878 --live-reload false",
    "openapi:contribuyente": "ng-openapi-gen -i openapi-gen/contribuyente.json -o src/app/api/contribuyente",
    "openapi:datoscomunes": "ng-openapi-gen -i openapi-gen/datoscomunes.json -o src/app/api/datoscomunes",
    "openapi:ubigeo": "ng-openapi-gen -i openapi-gen/ubigeo.json -o src/app/api/ubigeo",
    "openapi:vias": "ng-openapi-gen -i openapi-gen/vias.json -o src/app/api/vias"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.1.0",
    "@angular/cdk": "^14.2.2",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "@ng-bootstrap/ng-bootstrap": "^13.1.0",
    "@ng-select/ng-select": "^9.0.2",
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.2",
    "ng-openapi-gen": "^0.23.0",
    "ngx-mask": "^14.2.4",
    "rxjs": "~7.5.0",
    "single-spa": ">=4.0.0",
    "single-spa-angular": "^7.1.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "14.0.1",
    "@angular-devkit/build-angular": "^14.1.2",
    "@angular/cli": "~14.1.2",
    "@angular/compiler-cli": "^14.1.0",
    "@types/jasmine": "~4.0.0",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "style-loader": "^3.3.1",
    "typescript": "~4.7.2"
  }
}
